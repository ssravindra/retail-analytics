{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory4rproject1"
		},
		"LS_ADF_FILESYSTEM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ADF_FILESYSTEM'"
		},
		"LS_ADF_MYSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ADF_MYSQL'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_ADF_FILESYSTEM_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\sairavindra\\Sales_Data"
		},
		"LS_ADF_FILESYSTEM_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "RAVINDRA\\sairavindra"
		},
		"LS_ADF_MYSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"LS_ADF_MYSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "retailanalytics"
		},
		"LS_ADF_MYSQL_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"LS_ADF_REST_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://my-json-server.typicode.com/ssravindra/data/products"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retailanalyticsp1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory4rproject1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_ADF_FILESYSTEM_password": {
						"value": "[parameters('LS_ADF_FILESYSTEM_password')]"
					},
					"LS_ADF_MYSQL_password": {
						"value": "[parameters('LS_ADF_MYSQL_password')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_ADF_FILESYSTEM_properties_typeProperties_host": {
						"value": "[parameters('LS_ADF_FILESYSTEM_properties_typeProperties_host')]"
					},
					"LS_ADF_FILESYSTEM_properties_typeProperties_userId": {
						"value": "[parameters('LS_ADF_FILESYSTEM_properties_typeProperties_userId')]"
					},
					"LS_ADF_MYSQL_properties_typeProperties_server": {
						"value": "[parameters('LS_ADF_MYSQL_properties_typeProperties_server')]"
					},
					"LS_ADF_MYSQL_properties_typeProperties_database": {
						"value": "[parameters('LS_ADF_MYSQL_properties_typeProperties_database')]"
					},
					"LS_ADF_MYSQL_properties_typeProperties_username": {
						"value": "[parameters('LS_ADF_MYSQL_properties_typeProperties_username')]"
					},
					"LS_ADF_REST_properties_typeProperties_url": {
						"value": "[parameters('LS_ADF_REST_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}